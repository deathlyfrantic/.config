#!/usr/bin/env zsh
if [[ -n "$*" ]]; then
    browser="$*"
else
    browser="Safari"
fi
script=$(<<EOF
tell application "System Events"
    if (name of processes) contains "$browser" then
        tell application "$browser" to activate
        keystroke "n" using command down
    else
        tell application "$browser" to activate
    end if
end tell
EOF
)
osascript -e "$script"
